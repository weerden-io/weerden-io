{
  "name": "weerden-io",
  "version": "1.0.0",
  "description": "weerden io website",
  "engines": {
    "node": "16.x",
    "yarn": "1.x"
  },
  "main": "server/index.ts",
  "scripts": {
    "start": "ts-node -T server/index.ts",
    "start-dev": "npm-run-all --parallel start-frontend-dev start-server-dev",
    "start-frontend-dev": "cd frontend && ng serve",
    "start-server-dev": "ts-node-dev --respawn --transpile-only server/index.ts",
    "server-init": "yarn && yarn post-install",
    "post-install": "yarn write-version && yarn build-frontend",
    "write-version": "ts-node scripts/write-commit-to-version.ts",
    "build-frontend": "cd frontend && yarn && npm run build-prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weerden-io/weerden-io.git"
  },
  "author": "Jimenez Weerden",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/weerden-io/weerden-io/issues"
  },
  "homepage": "https://github.com/weerden-io/weerden-io#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^6.5.1",
    "express-sslify": "^1.2.0",
    "http-status": "^1.5.0",
    "rss-parser": "^3.9.0",
    "ts-node": "10.9.1",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.8",
    "@types/express-sslify": "^1.2.1",
    "@types/node": "^18.16.3",
    "npm-run-all": "^4.1.5",
    "ts-node-dev": "^2.0.0-0"
  }
}
